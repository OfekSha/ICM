<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="168.0" prefWidth="366.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.DocmentTableForDownloadsForm">
   <children>
      <Label text="Attached files" />
      <TableView fx:id="tblViewDocuments" prefHeight="120.0" prefWidth="172.0">
         <columns>
            <TableColumn fx:id="columnFileName" prefWidth="85.6666259765625" text="File name" />
            <TableColumn fx:id="columnFileSize" prefWidth="85.0" text="size (Mb)" />
         </columns>
      </TableView>
      <Button fx:id="btnDownload" mnemonicParsing="false" onAction="#pressedDownload" text="Download" />
   </children>
</VBox>
