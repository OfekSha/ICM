<?xml version="1.0" encoding="UTF-8"?>

<!--THIS IS BASIC ALMOST EMPTY FORM, PLEASE USE IT TO BUILD YOURS
DO NOT TOUCH: Exit & Logout buttons-->

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.UserForm">
   <top>
      <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <ImageView fitHeight="200.0" fitWidth="200.0">
                <Image url="@../img/ICMlogo.png" />
            </ImageView>
            <VBox alignment="TOP_RIGHT" prefHeight="200.0" HBox.hgrow="ALWAYS">
                  <Button fx:id="btnExit" mnemonicParsing="false" onAction="#ExitBtn" text="Exit">
                     <VBox.margin>
                        <Insets right="20.0" top="20.0" />
                     </VBox.margin>
                  </Button>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
      </HBox>
   </top>
   <center>
      <SplitPane dividerPositions="0.2" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="568.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
            <VBox alignment="TOP_CENTER" maxWidth="-Infinity" prefHeight="200.0" prefWidth="200.0">
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                        <Button mnemonicParsing="false" prefWidth="175.0" text="&lt;Something 0&gt;">
                           <VBox.margin>
                              <Insets bottom="20.0" top="30.0" />
                           </VBox.margin>
                        </Button>
                        <Button disable="true" mnemonicParsing="false" prefWidth="175.0" text="&lt;Something 1&gt;">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button disable="true" mnemonicParsing="false" prefWidth="175.0" text="&lt;Something N&gt;">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin>
                        </Button>
            </VBox>
            <VBox alignment="BOTTOM_CENTER" prefHeight="70.0">
               <VBox.margin>
                  <Insets top="300.0" />
               </VBox.margin>
                  <Button mnemonicParsing="false" text="Logout">
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </Button>
            </VBox>
            </VBox>
            <HBox fx:id="makeRequestBox" alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="100.0" prefWidth="200.0">
            <VBox alignment="TOP_CENTER" prefHeight="349.0" prefWidth="172.0">
               <VBox alignment="TOP_CENTER" prefHeight="300.0" prefWidth="172.0">
                        <Label fx:id="InformationSystem" alignment="CENTER" contentDisplay="BOTTOM" prefHeight="58.0" prefWidth="172.0" text="Information System" visible="false">
                           <VBox.margin>
                              <Insets top="15.0" />
                           </VBox.margin>
                        <graphic>
                              <ComboBox prefWidth="150.0" />
                        </graphic>
                        </Label>
                     <Label fx:id="RequestID" contentDisplay="BOTTOM" text="Request ID:">
                        <VBox.margin>
                           <Insets top="10.0" />
                        </VBox.margin>
                        <graphic>
                           <ComboBox prefWidth="150.0" />
                        </graphic>
                     </Label>
                     <Button fx:id="watchRequests" mnemonicParsing="false" text="Watch Submitted Requests" textAlignment="CENTER" wrapText="true">
                        <VBox.margin>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </VBox.margin>
                     </Button>
               </VBox>
               <VBox alignment="BOTTOM_CENTER" prefHeight="270.0" prefWidth="172.0">
                  <VBox.margin>
                     <Insets />
                  </VBox.margin>
               </VBox>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="600.0">
                  <Label contentDisplay="BOTTOM" text="Change request details">
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  <graphic>
                        <TextArea fx:id="taRequestDetails" prefHeight="100.0" wrapText="true">
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </TextArea>
                  </graphic>
                  </Label>
                  <Label alignment="CENTER" contentDisplay="BOTTOM" text="Change request reason">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  <graphic>
                        <TextArea fx:id="taRequestReason" prefHeight="100.0" wrapText="true" />
                  </graphic>
                  </Label>
                  <Label alignment="CENTER" contentDisplay="BOTTOM" text="Your comment (optional)">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  <graphic>
                        <TextArea fx:id="taComment" prefHeight="100.0" wrapText="true" />
                  </graphic>
                  </Label>
                  <Button fx:id="btnSubmit" contentDisplay="CENTER" mnemonicParsing="false" text="Submit Change Request" visible="false">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Button>
            </VBox>
            </HBox>
      </SplitPane>
   </center>
</BorderPane>