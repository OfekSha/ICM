<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="200.0" prefWidth="700.0" spacing="10.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.PopUpWindows.PerformanceReport">
     <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" text="Performance Report">
         <font>
             <Font size="24.0" />
         </font>
         <VBox.margin>
             <Insets />
         </VBox.margin>
     </Text>
   <VBox prefHeight="200.0" prefWidth="450.0">

         <Text fx:id="txtReturns" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Time Added Because of Returns:">
            <font>
               <Font size="30.0" />
            </font>
         </Text>
         <Text fx:id="txtExtensions" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Time Added From Extensions:">
            <font>
               <Font size="30.0" />
            </font>
            <VBox.margin>
               <Insets top="20.0" />
            </VBox.margin>
         </Text>
      <VBox.margin>
         <Insets right="150.0" />
      </VBox.margin>
   </VBox>
    <padding>
        <Insets top="10.0" />
    </padding>
</VBox>
