<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" fillWidth="false" spacing="10.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.PopUpWindows.ExceedWarning">
    <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: bold;" text="Warning!">
        <font>
            <Font size="24.0" />
        </font>
        <VBox.margin>
            <Insets />
        </VBox.margin>
    </Text>

     <Text fx:id="txtWarning" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
             <Font size="30.0" />
         </font>
     </Text>
   <TableView fx:id="tblRequests" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="colID" prefWidth="99.0" text="Request ID" />
       <TableColumn fx:id="colDueTime" prefWidth="100.0" text="Due Time" />
     </columns>
   </TableView>
   <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#getCancel" text="OK!">
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin>
   </Button>
    <padding>
        <Insets top="10.0" />
    </padding>
</VBox>
